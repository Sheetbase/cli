{
  "name": "@sheetbase/cli",
  "version": "3.0.0-beta1",
  "description": "Official CLI for working with Sheetbase.",
  "author": "Sheetbase",
  "homepage": "https://sheetbase.dev",
  "license": "MIT",
  "main": "src/public-api.js",
  "types": "src/public-api.d.ts",
  "bin": {
    "sheetbase": "src/bin.js"
  },
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile && npm i -g",
    "docs": "ayedocs generate",
    "test": "npm run compile && nyc --cache false mocha test/**/*.js",
    "coverage": "nyc --cache false report --reporter=text-lcov | coveralls",
    "lint": "gts lint",
    "fix": "gts fix"
  },
  "keywords": [
    "node",
    "module",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sheetbase/cli.git"
  },
  "bugs": {
    "url": "https://github.com/sheetbase/cli/issues"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "commander": "^6.1.0",
    "configstore": "^5.0.1",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "google-auth-library": "^6.1.2",
    "inquirer": "^7.3.3",
    "open": "^7.3.0",
    "randomstring": "^1.1.5",
    "tty-table": "^4.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^9.0.2",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/randomstring": "^1.1.6",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "gts": "^3.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "typescript": "^4.0.3"
  }
}
